{
  "name": "react-codemirror6",
  "version": "0.0.0-semantic",
  "description": "A wrapper around CodeMirror 6 so it behaves like a controlled input",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "umd": "dist/index.umd.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "coverage"
  ],
  "author": {
    "name": "Tom Golden",
    "email": "8162045+tbjgolden@users.noreply.github.com",
    "url": "https://github.com/tbjgolden"
  },
  "homepage": "https://github.com/tbjgolden/react-codemirror6",
  "repository": {
    "type": "git",
    "url": "https://github.com/tbjgolden/react-codemirror6.git"
  },
  "scripts": {
    "build": "yarn run clean && tsc -p tsconfig.build.json && rollup -c ./config/rollup.config.js && del compiled",
    "clean": "concurrently \"del compiled\" \"del coverage\" \"del dist\"",
    "ctrl": "ctrl",
    "lint": "eslint . --ext .ts,.tsx",
    "start": "start-storybook",
    "test": "concurrently \"yarn:lint\" \"yarn:build\"",
    "docs": "build-storybook -o docs",
    "prepare": "husky install"
  },
  "keywords": [
    "typescript"
  ],
  "license": "MIT",
  "bugs": {
    "url": "github:tbjgolden/react-codemirror6/issues"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.8",
    "@codemirror/autocomplete": "^0.19.15",
    "@codemirror/closebrackets": "^0.19.1",
    "@codemirror/commands": "^0.19.8",
    "@codemirror/comment": "^0.19.1",
    "@codemirror/fold": "^0.19.3",
    "@codemirror/gutter": "^0.19.9",
    "@codemirror/highlight": "^0.19.8",
    "@codemirror/history": "^0.19.2",
    "@codemirror/language": "^0.19.10",
    "@codemirror/lint": "^0.19.6",
    "@codemirror/matchbrackets": "^0.19.4",
    "@codemirror/rectangular-selection": "^0.19.2",
    "@codemirror/search": "^0.19.9",
    "@codemirror/state": "^0.19.9",
    "@codemirror/view": "^0.19.48"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@codemirror/theme-one-dark": "^0.19.1",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/react": "^6.3.6",
    "@types/dedent": "^0.7.0",
    "@types/mkdirp": "^1.0.2",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "babel-core": "6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "classnames": "^2.3.1",
    "commitizen": "^4.2.4",
    "concurrently": "^6.2.0",
    "cp-cli": "2.0.0",
    "cross-env": "7.0.3",
    "ctrl-scripts": "^0.1.0",
    "dedent": "^0.7.0",
    "del-cli": "4.0.1",
    "enquirer": "^2.3.6",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fast-glob": "^3.2.7",
    "fork-ts-checker-webpack-plugin": "^6.2.13",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.0",
    "mkdirp": "^1.0.4",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "0.13.9",
    "rollup": "2.54.0",
    "rollup-plugin-filesize": "9.1.1",
    "rollup-plugin-terser": "7.0.2",
    "run-parallel": "^1.2.0",
    "semantic-release": "^17.4.4",
    "stmux": "^1.8.1",
    "storybook": "^6.3.6",
    "ts-loader": "^9.2.4",
    "ts-node": "^10.1.0",
    "typescript": "4.3.5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "quoteProps": "consistent",
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "proseWrap": "always",
    "htmlWhitespaceSensitivity": "strict"
  },
  "eslintConfig": {
    "root": true,
    "extends": "./config/eslint.config.js"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
